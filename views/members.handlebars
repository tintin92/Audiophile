<div class="memberpageDiv m-4">
  <div class="row">
    <div class="col-sm-4">
      <div class="card boxColor">
        <div class="card-body">
          <h5 class="card-title" style="color: white;"><strong>Saved Artists</strong></h5>
            <div class="card">
              <div class="card-body">
                <p class="card-text">
                <ul>
            {{artist}}
                  <button class="memberDeleteBtn" data-id="{{this.id}}">Delete</button>
                </ul>
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-sm-4">
      <div class="card boxColor">
        <div class="card-body">
          <h5 class="card-title" style="color: white;"><strong>Saved Songs</strong></h5>
          <div class="card">
            <div class="card-body">
              <p class="card-text">
              <ul>
                <li>Saved song 1
                  <button class="memberSaveBtn">Later</button>
                  <button class="memberDeleteBtn">Delete</button>
                </li>
              </ul>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="col-sm-4">
      <div class="card boxColor">
        <div class="card-body">
          <h5 class="card-title" style="color: white;"><strong>Listen To Later</strong></h5>
          <div class="card">
            <div class="card-box">
              <p class="card-text">
              <ul>
                <li>Songs for later 1
                  <button class="memberSaveBtn">Save</button>
                  <button class="memberDeleteBtn">Delete</button>
                </li>
              </ul>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="card mt-4 boxColor">
    <div class="card-body">
      <h5 style="color: white;"><strong>Band Merchandise (WIP)</strong></h5>
      <div class="card-columns">
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Shirts</h5>
          </div>
        </div>
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Vinyls</h5>
          </div>
        </div>
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Posters</h5>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="card mt-4 boxColor">
    <div class="card-body">
      <h5 style="color: white;"><strong>Upcoming Events (WIP)</strong></h5>
      <div class="card-columns">
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Shows</h5>
          </div>
        </div>
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Tour Dates</h5>
          </div>
        </div>
        <div class="card text-center">
          <div class="card-body">
            <h5 class="card-title">Artist Website</h5>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
